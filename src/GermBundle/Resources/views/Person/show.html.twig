{% extends 'GermBundle::base.html.twig' %}

{% block content_content %}
	<h1>{{ person.name }}</h1>
	<dl>
	  <dt>{% trans %}Phone{% endtrans %}</dt>
	  <dd>{{ person.phone|join('<br />') }}</dd>
	  <dt>{% trans %}Address{% endtrans %}</dt>
	  <dd>{{ person.address|nl2br }}</dd>
	</dl>
{% endblock %}


